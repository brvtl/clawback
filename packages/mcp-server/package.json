{
  "name": "clawback-mcp",
  "version": "0.1.0",
  "description": "MCP server for Clawback - event-driven Claude automation engine",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./tools": {
      "types": "./dist/tools.d.ts",
      "default": "./dist/tools.js"
    }
  },
  "bin": {
    "clawback-mcp": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brvtl/clawback.git",
    "directory": "packages/mcp-server"
  },
  "keywords": [
    "mcp",
    "mcp-server",
    "claude",
    "anthropic",
    "automation",
    "model-context-protocol"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "prepublishOnly": "tsc"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0"
  },
  "devDependencies": {
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
